                <ul class="nav navbar-nav">
                    <li id="start" class="active"><a href="#" style="font-size:25px;">↖</a></li>
                    <li id="HResistor"><a href="#">@Resources.Resource.Resistor</a></li>
                    <li id="HCapasitor"><a href="#">@Resources.Resource.Capasitor</a></li>
                    <li id="HTransistor"><a href="#">@Resources.Resource.Transistor</a></li>
                    <li id="HGround"><a href="#">@Resources.Resource.Ground</a></li>
                    <li id="HSource"><a href="#">@Resources.Resource.Source</a></li>
                    <li id="HInduct"><a href="#">@Resources.Resource.Induct</a></li>
